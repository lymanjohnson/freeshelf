<%= link_to 'Add a Book' , new_book_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th>Genre</th>
    <th>Year</th>
    <th>Description</th>
  </tr>

  <%@books.each do |book|%>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.author %></td>
      <td><%= book.genre %></td>
      <td><%= book.year %></td>
      <td><%= book.description %></td>
    </tr>
  <% end %>
</table>
